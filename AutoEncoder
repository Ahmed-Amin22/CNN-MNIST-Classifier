    {
      "cell_type": "code",
      "source": [
        "#Model (encoding , bottel neck , decoding)\n",
        "input_layer=Input(shape=(x_train.shape[1],))\n",
        "bottelneck = Dense(32, activation='relu' , name ='bottelneck')(input_layer)\n",
        "output=Dense(x_train.shape[1],activation='sigmoid',name='output')(bottelneck)\n"
      ],
      "metadata": {
        "id": "6RUeNoiDNCgq"
      },
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "autoencoder=Model(input_layer ,output)\n",
        "autoencoder.summary()"
      ],
